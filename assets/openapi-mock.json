{
    "openapi": "3.0.3",
    "info": {
        "title": "Client Generator Test API",
        "version": "1.0.0"
    },
    "paths": {
        "/items": {
            "get": {
                "summary": "List items",
                "responses": {
                    "200": {
                        "description": "A list of items.",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ItemListResponse"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "summary": "Create an item",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ItemCreate"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Item"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/items/{item_id}": {
            "get": {
                "summary": "Get item by ID",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Item"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/endpoint/with-description": {
            "get": {
                "summary": "This endpoint includes a description",
                "description": "This is an endpoint with a description included.",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Item"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/bad-path/method/no-summary": {
            "get": {
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "The item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Item"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/bad-path/method/no_2xx_response_available": {
            "get": {
                "description": "This specific endpoint does not include a response within the 2xx. Which is normally used to determine the response type.",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/bad-path/parameters/no_schema_attached": {
            "get": {
                "description": "This checks the handling of a parameter without a schema object.",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/good-path/parameters/query_parameter_as_ref": {
            "get": {
                "description": "This checks the handling of a parameter that is a query parameter with a ref.",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "$ref": "#/components/schemas/Item"
                        }
                    }
                ],
                "responses": {
                    "404": {
                        "description": "Item not found"
                    }
                }
            }
        },
        "/response/single_ref": {
            "get": {
                "summary": "Simple 200 with single schema ref",
                "responses": {
                    "200": {
                        "description": "Single Item",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Item"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/response/array_of_ref": {
            "get": {
                "summary": "200 returning an array of Item",
                "responses": {
                    "200": {
                        "description": "List of Items",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Item"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/response/array_of_primitive": {
            "get": {
                "summary": "200 returning an array of primitive values",
                "responses": {
                    "200": {
                        "description": "List of primitive values",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/response/no_schema_in_2xx": {
            "get": {
                "summary": "200 response without a schema/content",
                "responses": {
                    "200": {
                        "description": "Success without schema"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Item": {
                "type": "object",
                "required": [
                    "id",
                    "name"
                ],
                "properties": {
                    "id": {
                        "type": "string",
                        "example": "item_123",
                        "max_length": 100,
                        "title": "Test"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    }
                }
            },
            "ItemCreate": {
                "type": "object",
                "required": [
                    "name"
                ],
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    }
                }
            },
            "ItemListResponse": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Item"
                        }
                    },
                    "total": {
                        "type": "integer",
                        "example": 1
                    }
                }
            },
            "ComplexArray": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Item"
                        }
                    }
                }
            },
            "OneOfSchema": {
                "type": "object",
                "properties": {
                    "value": {
                        "oneOf": [
                            {
                                "$ref": "#/components/schemas/Item"
                            },
                            {
                                "$ref": "#/components/schemas/ItemCreate"
                            }
                        ]
                    }
                }
            },
            "AnyOfSchema": {
                "type": "object",
                "properties": {
                    "value": {
                        "anyOf": [
                            {
                                "$ref": "#/components/schemas/Item"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    }
                }
            },
            "AllOfSchema": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/Item"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "extra": {
                                "type": "string"
                            }
                        }
                    }
                ]
            },
            "NestedListSchema": {
                "type": "object",
                "properties": {
                    "list": {
                        "type": "array",
                        "items": [
                            {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/ItemCreate"
                                }
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "deep": {
                                        "$ref": "#/components/schemas/ItemListResponse"
                                    }
                                }
                            }
                        ]
                    }
                }
            },
            "SelfRefSchema": {
                "type": "object",
                "properties": {
                    "self": {
                        "$ref": "#/components/schemas/SelfRefSchema"
                    }
                }
            },
            "ItemReservedKeyword": {
                "type": "object",
                "properties": {
                    "from": {
                        "type": "string",
                        "example": "item_123"
                    }
                }
            },
            "ItemCreateAlias": {
                "type": "object",
                "properties": {
                    "PascalCase": {
                        "type": "string",
                        "example": "item_123"
                    }
                }
            },
            "ItemEmptyArray": {
                "type": "object",
                "properties": {
                    "list": {
                        "type": "array",
                        "items": []
                    }
                }
            },
            "EmptyProperty": {
                "type": "object",
                "properties": {
                    "empty_value": null
                }
            },
            "TimeValidity": {
                "developers_note": "Contains a schema where it only contains a primitive type including a enum",
                "description": "Specifies how long the order remains active: \n* DAY: The order will automatically expire if not executed by midnight in the time zone of the instrument's exchange.\n* GOOD_TILL_CANCEL: The order remains active indefinitely until it is either filled or explicitly cancelled by you.",
                "enum": [
                    "DAY",
                    "GOOD_TILL_CANCEL"
                ],
                "type": "string"
            }
        }
    }
}